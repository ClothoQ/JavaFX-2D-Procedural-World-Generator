<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="Texture" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@textures/grass.png" />
         </image>
      </ImageView>
      <Label fx:id="ID" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="28.0" text="3" textFill="#c8c8c8" HBox.hgrow="ALWAYS" />
      <Label fx:id="HIGH" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="28.0" text="20" textFill="#c8c8c8" HBox.hgrow="ALWAYS" />
      <Label fx:id="LOW" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="28.0" text="20" textFill="#c8c8c8" HBox.hgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="2.0" top="2.0" />
   </padding>
</HBox>
